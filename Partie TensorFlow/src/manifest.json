{
  "name": "SHAMEBOT",
  "version": "0.1",
  "description": "Gives the average pornscore of all the images in page and sends it to Discord",
  "manifest_version": 2,
  "homepage_url": "https://github.com/whiterqbbit/Shamebot",
  "permissions": ["activeTab", "contextMenus", "storage", "identity", "https://discordapp.com/api/"],
  "background": {
    "scripts": ["nsfwjs.min.js", "background.js"]
  },
  "content_security_policy": "script-src 'self' filesystem: http://localhost:* https://unpkg.com/* 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}