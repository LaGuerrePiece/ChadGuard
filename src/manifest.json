  {
    "name": "CHADGUARD",
    "version": "0.1",
    "description": "For now : send to discord when click on 'I watch p0rn'.",
    "manifest_version": 2,
	"homepage_url": "https://github.com/whiterqbbit/Shamebot",
    "permissions": ["contextMenus", "storage", "identity", "https://discordapp.com/api/"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png"
      },
      "default_popup": "popup.html"
    }
  }