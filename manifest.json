{
  "name": "CHADGUARD",
  "version": "1.0",
  "description": "Reveal your inner chad. CHADGUARD keeps you away from porn.",
  "manifest_version": 2,
  "homepage_url": "https://github.com/whiterqbbit/Shamebot",
  "permissions": ["activeTab", "storage", "identity", "https://discordapp.com/api/"],
  "background": {
    "scripts": ["nsfwjs.min.js", "background.js"]
  },
  "content_security_policy": "script-src 'self' filesystem: http://localhost:* https://unpkg.com/* 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "./images/icon16.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "./images/icon16.png",
      "48": "./images/icon48.png",
      "128": "./images/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["nsfwjs.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    "urls.json",
    "blockpage.html"
  ]
}